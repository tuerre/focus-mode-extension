{
    "manifest_version": 3,
    "name": "ASD Focus Mode",
    "description": "Activa el Focus Mode para tomar un descanso de las redes sociales y eliminar todas tus distracciones.",
    "version": "1.0",
    "permissions": [
        "storage",
        "tabs"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icon.png",
            "48": "icon.png",
            "128": "icon.png"
        }
    },
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    }
}